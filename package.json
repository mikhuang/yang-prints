    "@reach/router": "^1.2.1",
    "@sentry/browser": "^5.6.3",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "fuse.js": "^3.4.5",
    "jss": "10.0.0-alpha.24",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.6.0",
    "react-hotkeys-hook": "^1.5.3",
    "react-jss": "10.0.0-alpha.24",
    "react-lazy-load-image-component": "^1.3.2",
    "react-static": "^7.2.0",
    "react-static-plugin-jss": "^7.2.2",
    "react-static-plugin-reach-router": "^7.2.0",
